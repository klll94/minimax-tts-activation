{
  "functions": {
    "api/validate.py": {
      "runtime": "@vercel/python"
    },
    "api/status.py": {
      "runtime": "@vercel/python"
    }
  },
  "routes": [
    {
      "src": "/api/validate",
      "dest": "/api/validate.py"
    },
    {
      "src": "/api/status", 
      "dest": "/api/status.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
